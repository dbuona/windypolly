@article{Ollerton:2011aa,
	Annote = {doi: 10.1111/j.1600-0706.2010.18644.x},
	Author = {Ollerton, Jeff and Winfree, Rachael and Tarrant, Sam},
	Booktitle = {Oikos},
	Da = {2011/03/01},
	Date = {2011/03/01},
	Date-Added = {2020-06-25 12:42:45 -0400},
