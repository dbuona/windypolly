	Doi = {https://doi.org/10.1016/j.agrformet.2020.108042},
	Isbn = {0168-1923},
	Journal = {Agricultural and Forest Meteorology},
	Keywords = {Aerobiology; Meteorological conditions; Precipitation; Thunderstorm},
	Pages = {108042},
	Title = {The impact of rainfall on the diurnal patterns of atmospheric pollen concentrations},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/pii/S0168192320301441},
	Volume = {291},
	Year = {2020},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0168192320301441},
