@article{Koenig:2015aa,
	Annote = {doi: 10.1890/14-0819.1},
	Author = {Koenig, Walter D. and Knops, Johannes M. H. and Carmen, William J. and Pearse, Ian S.},
	Booktitle = {Ecology},
