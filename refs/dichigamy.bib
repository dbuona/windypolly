@article{MUNGUIA-ROSAS:2011aa,
	Annote = {doi: 10.1111/j.1442-1984.2011.00320.x},
	Author = {MUNGUIA-ROSAS, MIGUEL A. and OLLERTON, JEFF and PARRA-TABLA, VICTOR},
	Booktitle = {Plant Species Biology},
	Da = {2011/09/01},
	Date = {2011/09/01},
	Date-Added = {2020-06-04 13:51:42 -0400},
